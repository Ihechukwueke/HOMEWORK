type:a,value:c},{type:b,tag:h,props:{id:v},children:[{type:b,tag:f,props:{href:Q,ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:w}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:aV}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"You will be an important part of our delivery team, working directly with clients, designers, strategists and developers to ensure that the work we produce meets and surpasses user needs and expectations."}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"This role would be ideal for someone who is passionate about how great digital products, tools, and experiences can benefit users and organisations alike."}]},{type:a,value:c},{type:b,tag:h,props:{id:x},children:[{type:b,tag:f,props:{href:R,ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:y}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:S}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:T}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"We work with some of the most exciting and interesting cultural organisations in the world including the National Theatre, Shakespeare’s Globe, Sadler’s Wells, Santa Fe Opera, Royal Court Theatre, American Repertory Theater, Bridge Theatre, Malmö Live and Malmö Opera amongst many others. Read more about "},{type:b,tag:f,props:{href:U,rel:[o,p,q],target:r},children:[{type:a,value:V}]},{type:a,value:W},{type:b,tag:f,props:{href:X,rel:[o,p,q],target:r},children:[{type:a,value:Y}]},{type:a,value:u}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"We also provide consultancy services and training on analytics, digital strategy, content, ticketing, project management and digital community events."}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:Z}]},{type:a,value:c},{type:b,tag:h,props:{id:J},children:[{type:b